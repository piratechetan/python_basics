    "def Reverse(num):\n",
    "    rev = 0\n",
    "    while num>0:\n",
    "        \n",
    "        rev = rev*10 + num%10\n",
    "        num=num//10\n",
    "    return rev\n",
    "num = int(input(\"Enter interger\"))\n",
    "value = Reverse(num)\n",
    "print(\"Reverse Interger is \",value)"
